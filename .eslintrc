{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "indent": ["error", 4],
        "object-curly-spacing": [2, "always"],
        "require-jsdoc": "off",
        "operator-linebreak": "off",
        "space-before-function-paren": "off",
        "semi": "off",
        "no-extra-semi": "off"
        // "no-tabs": "off"
    },
    "extends": ["eslint:recommended", "google"]
}
